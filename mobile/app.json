{
  "expo": {
    "name": "mobile-aws-vpn-connector",
    "slug": "mobile-aws-vpn-connector",
    "scheme": "myapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.krasovsky.vpnmanager",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "aws": {
        "cognito_region": "",
        "cognito_userPoolId": "",
        "cognito_userPoolWebClientId": "",
        "cognito_mandatorySignIn": true,
        "appsync_graphqlEndpoint": "",
        "appsync_region": "",
        "appsync_authenticationType": ""
      },
      "eas": {
        "projectId": "9a761278-4e88-4845-b96f-390bf4f3f55d"
      }
    },
    "owner": "krasovsky",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/9a761278-4e88-4845-b96f-390bf4f3f55d"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ]
  }
}
